<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Quetionnaire"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        granite:class="cmp-button__editor"
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs">
                <items jcr:primaryType="nt:unstructured">
                    <screen-1
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Screen 1"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <questions
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Rank Options"
                                                fieldLabel="Use Case Ranking"
                                                name="./question"/>
                                            <stepOneNextButtonCtaLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Please enter label of next button cta"
                                                fieldLabel="Next Button Cta Label"
                                                name="./stepOneNextButtonCtaLabel"/>    
                                            <questionnaire_options_multifield
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                                composite="{Boolean}true"
                                                fieldDescription="Enter the options for all the fields&#xa; for first screen"
                                                fieldLabel="Types of Motorcycle Ranking">
                                                <field
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/fieldset"
                                                    fieldDescription="Enter the options for all the fields&#xa; for first screen"
                                                    name="./options">
                                                    <items jcr:primaryType="nt:unstructured">
                                                        <column
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/container">
                                                            <items jcr:primaryType="nt:unstructured">
                                                                <quetionnaire_options
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                    fieldLabel="Enter yours options"
                                                                    name="./answers"/>
                                                                <motorcycleRankingImage
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/pathbrowser"
                                                                    fieldLabel="Motorcycle Ranking Images"
                                                                    name="./motorcycleRankingImage"/>
                                                                <motorcycleRankingDesktopImage
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/pathbrowser"
                                                                    fieldLabel="Motorcycle Ranking Desktop Image"
                                                                    fieldDescription="Please select image path for motorcycle Ranking Desktop Image." 
                                                                    rootPath="/content/dam" 
                                                                    name="./motorcycleRankingDesktopImage"/>         
                                                            </items>
                                                        </column>
                                                    </items>
                                                </field>
                                            </questionnaire_options_multifield>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </screen-1>
                    <screen-2
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Screen 2"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <questions
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Frequency of Riding with Pillion"
                                                name="./question2"/>
                                            <stepTwoNextButtonCtaLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Please enter label of next button cta step two"
                                                fieldLabel="Next Button Cta Label"
                                                name="./stepTwoNextButtonCtaLabel"/>
                                            <stepTwoBackButtonCtaLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Please enter label of back button cta step two"
                                                fieldLabel="Back Button Cta Label"
                                                name="./stepTwoBackButtonCtaLabel"/>          
                                            <questionnaire_options_multifield
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                                composite="{Boolean}true"
                                                fieldDescription="Enter the Question for second screen"
                                                fieldLabel="Pillion Riding Frequency">
                                                <field
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/fieldset"
                                                    name="./screen2">
                                                    <items jcr:primaryType="nt:unstructured">
                                                        <column
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/container">
                                                            <items jcr:primaryType="nt:unstructured">
                                                                <quetionnaire_options
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                    fieldLabel="Enter your options"
                                                                    name="./checkbox"/>
                                                                <pillionFrequencyImage
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/pathbrowser"
                                                                    fieldLabel="Pillion Frequency Image"
                                                                    fieldDescription="Please select image path for pillion frequency mobile Image." 
                                                                    rootPath="/content/dam" 
                                                                    name="./pillionFrequencyImage"/>
                                                                <pillionFrequencyDesktopImage
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/pathbrowser"
                                                                    fieldLabel="Pillion Frequency Desktop Image"
                                                                    fieldDescription="Please select image path for pillion frequency Desktop Image." 
                                                                    rootPath="/content/dam" 
                                                                    name="./pillionFrequencyDesktopImage"/>       
                                                            </items>
                                                        </column>
                                                    </items>
                                                </field>
                                            </questionnaire_options_multifield>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </screen-2>
                    <screen-3
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Screen 3"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <questions
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Use Case Ranking"
                                                name="./question3"/>
                                             <stepThreeNextButtonCtaLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Please enter label of next button cta step three"
                                                fieldLabel="Next Button Cta Label"
                                                name="./stepThreeNextButtonCtaLabel"/>
                                            <stepThreeBackButtonCtaLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Please enter label of back button cta step three"
                                                fieldLabel="Back Button Cta Label"
                                                name="./stepThreeBackButtonCtaLabel"/>     
                                            <questionnaire_options_multifield
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                                composite="{Boolean}true"
                                                fieldDescription="This description will be displayed as content on the page and should provide details or context related to the image."
                                                fieldLabel="Types of Motorcycle Ranking">
                                                <field
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/fieldset"
                                                    name="./screen3">
                                                    <items jcr:primaryType="nt:unstructured">
                                                        <column
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/container">
                                                            <items jcr:primaryType="nt:unstructured">
                                                                <quetionnaire_options
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                    fieldLabel="Enter your options"
                                                                    name="./option3"/>
                                                                <background-images
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/pathbrowser"
                                                                    fieldLabel="Select the Mobile  Image"
                                                                    fieldDescription="Please select image path for mobile Image." 
                                                                    rootPath="/content/dam" 
                                                                    name="./images"/>
                                                                <optionImageForDesktop
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/pathbrowser"
                                                                    fieldLabel="Select the Desktop  Image"
                                                                    fieldDescription="Please select image path for desktop Image." 
                                                                    rootPath="/content/dam" 
                                                                    name="./optionImageForDesktop"/>    
                                                            </items>
                                                        </column>
                                                    </items>
                                                </field>
                                            </questionnaire_options_multifield>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </screen-3>
                    <screen-4
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Screen 4"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        fieldDescription="This description will be displayed as content on the page and should provide details or context related to the image."
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <questions
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Preferred Riding Positions"
                                                name="./question4"/>
                                            <stepFourNextButtonCtaLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Please enter label of next button cta step four"
                                                fieldLabel="Next Button Cta Label"
                                                name="./stepFourNextButtonCtaLabel"/>
                                            <stepFourBackButtonCtaLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Please enter label of back button cta step four"
                                                fieldLabel="Back Button Cta Label"
                                                name="./stepFourBackButtonCtaLabel"/>    
                                            <questionnaire_options_multifield
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                                composite="{Boolean}true"
                                                fieldLabel="Choose Riding Position Images">
                                                <field
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/fieldset"
                                                    fieldLabel="Enter your question"
                                                    name="./screen4">
                                                    <items jcr:primaryType="nt:unstructured">
                                                        <column
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/container">
                                                            <items jcr:primaryType="nt:unstructured">
                                                                <quetionnaire_options
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/pathbrowser"
                                                                    fieldLabel="Enter your riding Position Showing Name"
                                                                    fieldDescription="Please select image path for Riding Position mobile Image." 
                                                                    rootPath="/content/dam" 
                                                                    name="./ridingPosition"/>
                                                                <ridingPositionDesktopImage 
                                                                    jcr:primaryType="nt:unstructured" 
                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/pathbrowser" 
                                                                    fieldDescription="Please select image path for Riding Position Desktop Image." 
                                                                    fieldLabel="Select Desktop Image" 
                                                                    name="./ridingPositionDesktopImage" 
                                                                    rootPath="/content/dam" />    
                                                                <ridingPositionShowingNameKey
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                    fieldLabel="Enter your riding Position Showing Name"
                                                                    name="./ridingPositionShowingNameKey"/>
                                                                <ridingPositionShowingNameValue
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                    fieldLabel="Enter your riding Position Value Name"
                                                                    name="./ridingPositionShowingNameValue"/>    
                                                            </items>
                                                        </column>
                                                    </items>
                                                </field>
                                            </questionnaire_options_multifield>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </screen-4>
                    <screen-5
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Screen 5"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <questions
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Perosnal Interest "
                                                name="./question5"/>
                                            <stepFiveNextButtonCtaLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Please enter label of next button cta step five"
                                                fieldLabel="Next Button Cta Label"
                                                name="./stepFiveNextButtonCtaLabel"/>
                                            <stepFiveBackButtonCtaLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Please enter label of back button cta step five"
                                                fieldLabel="Back Button Cta Label"
                                                name="./stepFiveBackButtonCtaLabel"/>     
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </screen-5>
                    <icons
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Icons"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <select-icon
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/pathbrowser"
                                                fieldDescription="Select the Icon for choose any  image"
                                                fieldLabel="Image picker Icon&#xa;"
                                                name="./selectIcon"/>
                                            <firstScreenImage
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/pathbrowser"
                                                fieldDescription="Upload the image that will be displayed on the first five screen. Ensure the image is designed specifically for the first five screen, as it may differ in layout or size."
                                                fieldLabel="Background Image"
                                                name="./baseImage"/>
                                            <lastScreenImage
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/pathbrowser"
                                                fieldDescription="Upload the image that will be displayed on the last screen. Ensure the image is designed specifically for the last screen, as it may differ in layout or size."
                                                fieldLabel="Background Image for final screen"
                                                name="./finalBGImage"/>
                                            <draggable-icon
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/pathbrowser"
                                                fieldDescription="Select the Icon for drag  images"
                                                fieldLabel="Draggable Icon"
                                                name="./draggableIcon"/>
                                            <lastScreenImage-for-mobile
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/pathbrowser"
                                                fieldDescription="Upload the image that will be displayed on the last screen. Ensure the image is designed specifically for the last screen, as it may differ in layout or size."
                                                fieldLabel="Background Image for mobile final screen"
                                                name="./finalBGImageForMobile"/>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </icons>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
